  	<!-- 消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 -->
	<dubbo:application name="msc-provider" logger="slf4j" />
 	<!-- 使用zookeeper注册中心暴露服务地址 -->
	<dubbo:registry protocol="zookeeper" address="${dubbo.registry.address}" file="${dubbo.registry.file.b2b}"/>
	
    <!-- 用dubbo协议在20880端口暴露服务 -->
    <dubbo:protocol name="dubbo" port="${dubbo.protocol.b2b.port}" accepts="500" iothreads="500" threads="500"  />
 	<!-- 监控中心配置 --> 
	<!-- <dubbo:monitor protocol="registry"/> -->

    <!--调整dubbo  请求及响应数据包大小限制，单位：字节-->
    <dubbo:provider id="payload" payload="11557050"/>
	
